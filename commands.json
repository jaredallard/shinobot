{
  "events": [{
    "interval": 900000,
    "type": "timer",
    "function": "postImage"
  },
  {
    "type": "one-shot",
    "target": "never",
    "function": "postImage"
  },
  {
    "type": "one-shot",
    "target": "init",
    "function": "updateLocation"
  },
  {
    "type": "filter",
    "on": "tweet",
    "function": "filterTweets"
  }],
  "commands": [{
    "stream": "mentions",
    "pattern": [{
      "string": "[a-z]+",
      "flag": "gi"
    },
    {
      "string": "version",
      "flag": "gi"
    }],
    "function": "getVersion",
    "response": "v: {{version}} c: {{commit}}",
    "matchAll": true
  },
  {
    "stream": "mentions",
    "pattern": {
      "string": "get(images?|imgs?)",
      "flag": "gi"
    },
    "function": "getImage"
  }],
  "streams": [
    "mentions",
    "home"
  ]
}
