{
  "events": [{
    "interval": 900000,
    "type": "timer",
    "function": "postImage"
  },
  {
    "type": "one-shot",
    "target": "never",
    "function": "postImage"
  },
  {
    "type": "one-shot",
    "target": "init",
    "function": "updateLocation"
  }],
  "commands": [{
    "stream": "mentions",
    "pattern": {
      "string": "version",
      "flag": "gi"
    },
    "function": "getVersion",
    "response": "v: {{version}} c: {{commit}}"
  },
  {
    "stream": "mentions",
    "order": 0,
    "pattern": {
      "string": "get(images?|imgs?)",
      "flag": "gi"
    },
    "function": "getImage"
  }],
  "streams": [
    "home",
    "mentions"
  ]
}
